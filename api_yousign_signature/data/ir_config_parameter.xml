<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- permet de ne plus modifier l'url automatiquement car elle contient http au lieu de https sur nestor -->
        <record model="ir.config_parameter" id="config_yousign_url_freeze">
            <field name="key">web.base.url.freeze</field>
            <field name="value">True</field>
        </record>

         <record model="ir.config_parameter" id="config_yousign_webhook">
            <field name="key">yousign_webhook</field>
            <field name="value">yousign_webhook_key</field>
        </record>

        <record model="ir.config_parameter" id="config_yousign_api_key">
            <field name="key">yousign_api_key</field>
            <field name="value" />
        </record>

        <record model="ir.config_parameter" id="config_yousign_staging">
            <field name="key">yousign_staging</field>
            <field name="value">True</field>
        </record>

    </data>
</odoo>