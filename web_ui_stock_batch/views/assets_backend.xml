<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <template id="assets_backend" name="stock assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/web_ui_stock_batch/static/src/js/batch_main_list.js"/>
                <script type="text/javascript" src="/web_ui_stock_batch/static/src/js/batch_main_list_move_line_row.js"/>
                <script type="text/javascript" src="/web_ui_stock_batch/static/src/js/batch_main_list_move_line_lot.js"/>
                <script type="text/javascript" src="/web_ui_stock_batch/static/src/js/batch_main_list_move_line_numpad.js"/>
                <script type="text/javascript" src="/web_ui_stock_batch/static/src/js/batch_main_list_selection.js"/>
            </xpath>
        </template>

        <template id="assets_backend_index_batch" name="stock assets index" inherit_id="web_ui_stock.index">
            <xpath expr="//head" position="inside">
                <link rel="stylesheet" href="/web_ui_stock_batch/static/src/css/theme.css"/>
            </xpath>
        </template>

    </data>
</odoo>